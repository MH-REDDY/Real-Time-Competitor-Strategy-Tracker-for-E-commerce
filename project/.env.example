# Copy this file to `project/.env` and fill in real values. Never commit the real `.env`.
# MongoDB
MONGO_URI="mongodb+srv://<username>:<password>@<cluster-host>/?appName=ecomtracker"
MONGO_DB="ecom_tracker"
MONGO_COLLECTION="synthetic_data"

# Razorpay (replace with your test/live keys)
RAZORPAY_KEY_ID=rzp_test_<your_key>
RAZORPAY_KEY_SECRET=<your_secret>
VITE_RAZORPAY_KEY_ID=rzp_test_<your_key>

# Slack webhook
SLACK_WEBHOOK="https://hooks.slack.com/services/<your>/<webhook>/<token>"
# Vite / Frontend example env file
# DO NOT commit real secrets here. Use project/.env for local dev only.
VITE_BACKEND_URL=http://localhost:8001
VITE_LLM_API_URL=http://localhost:5001
VITE_RAZORPAY_KEY_ID=rzp_test_PLACEHOLDER
# To mark any client-side key for dev only; do not store production keys here
VITE_API_KEY=
